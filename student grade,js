let grade=1

if (grade>=79 && grade<=100){
    console.log("Grade A")
}
else if (grade>=60 && grade<=79){
    console.log("Grade B")
}
else if (grade>=49 && grade<=59){
    console.log("Grade C")
}
else if (grade>=40 && grade<=49){
    console.log("Grade D")
}else if (grade>=0 && grade<=40){
    console.log("Grade E")
}

else{
    console.log("Invalid Input")
}